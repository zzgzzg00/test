{
  "name": "electorntest",
  "productName": "electorntest",
  "version": "1.0.0",
  "description": "Generate beautiful reading card in one click",
  "main": "index.js",
  "appId": "com.brickyang.rcgenerator",
  "build": {
    "appId": "com.brickyang.rcgenerator",
    "productName": "RC Generator",
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package": "electron-packager . --all",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win --x64",
    "dist-mac": "electron-builder --mac --x64"
  },
  "author": "zzg",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-builder": "^20.28.3",
    "electron-squirrel-startup": "^1.0.0",
    "grunt": "^1.0.3",
    "grunt-electron-installer": "^2.1.0"
  }
}
