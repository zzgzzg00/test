{
    "name":"test",
    "version":"1.0",
    "manifest_version":2,
    "browser_action":{
        "default_icon":"./x.png",
        "default_popup":"./popup.html"
    },
    "permissions":[
        "cookies",
        "https://touch.train.qunar.com/*",
        "https://suggest.taobao.com/",
        "tabs",
        "https://www.taobao.com/"
    ],
    "background": {
        "scripts": ["./background.js"]
    },
    "content_scripts": [{
            "matches": ["https://www.taobao.com/"],
            "css": ["./taobao/style.css"],
            "js": ["./taobao/taobao.js"],
            "run_at":"document_idle"
        },{
        "matches": ["https://s.taobao.com/*"],
        "css": ["./taobao/style.css"],
        "js": ["./taobao/taobao.js"],
        "run_at":"document_idle"
    }]
}